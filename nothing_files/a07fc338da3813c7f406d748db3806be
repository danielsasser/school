}

.edlInlineTMCE p,
.edlInlineTMCE ul,
.edlInlineTMCE ol
{
margin:16px 0 16px 0;
}

.edlInlineTMCE blockquote
{
margin:16px 40px 16px 40px;
}

.edlInlineTMCE ul,
.edlInlineTMCE ol
{
padding:0 0 0 40px;
}

